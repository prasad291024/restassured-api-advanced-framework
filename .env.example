# Environment Variables Template
# Copy this file to .env and fill in actual values
# DO NOT commit .env file to version control

# Authentication
AUTH_CLIENT_ID=your-client-id
AUTH_CLIENT_SECRET=your-client-secret-base64-encoded
AUTH_USERNAME=your-username
AUTH_PASSWORD=your-password-base64-encoded

# Proxy (if needed)
PROXY_USERNAME=your-proxy-username
PROXY_PASSWORD=your-proxy-password-base64-encoded

# SSL
SSL_KEYSTORE_PASSWORD=your-keystore-password-base64-encoded

# Environment Selection
TEST_ENV=qa
